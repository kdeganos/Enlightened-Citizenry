<h3>Bills</h3>
<table class="bills">
<tr>
    <th>Title</th>
    <th>Introduced</th>
    <th>Sponsor</th>
    <th>Chamber</th>
</tr>

{{#each model as |bill|}}
<tr>
  <td>{{bill.official_title}}</td>
  <td>{{bill.introduced_on}}</td>
  <td>{{bill.sponsor.first_name}} {{bill.sponsor.last_name}}</td>
  <td>{{bill.chamber}}</td>
</tr>
{{/each}}
</table>
